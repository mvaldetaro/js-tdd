{
  "name": "js-tdd",
  "version": "1.0.0",
  "description": "",
  "main": "./index.js",
  "scripts": {
    "build": "npm run clear && ./node_modules/.bin/babel --out-dir lib src",
    "build:watch": "npm run build -- --watch",
    "build:umd": "./node_modules/.bin/webpack --output-filename spotify-wrapper.umd.js",
    "build:umd:min": "./node_modules/.bin/webpack --output-filename spotify-wrapper.umd.min.js -p",
    "build:all": "npm run build && npm run build:umd && npm run build:umd:min",
    "clear": "rimraf lib",
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint && npm run coverage",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require @babel/register",
    "watch": "npm run test -- --watch",
    "coverage": "nyc npm test"
  },
  "nyc": {
    "functions": 80,
    "lines": 80,
    "check-coverage": true,
    "reporter": [
      "text",
      "html"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvaldetaro/js-tdd.git"
  },
  "author": "Magno Valdetaro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mvaldetaro/js-tdd/issues"
  },
  "homepage": "https://github.com/mvaldetaro/js-tdd#readme",
  "dependencies": {
    "node-fetch": "^3.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.8.3",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-mocha": "^6.2.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "mocha": "^6.2.2",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.1",
    "sinon": "^8.0.1",
    "sinon-chai": "^3.4.0",
    "sinon-stub-promise": "^4.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
