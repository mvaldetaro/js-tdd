{
  "name": "js-tdd",
  "version": "1.0.0",
  "description": "",
  "main": "./src/main.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint && npm run coverage",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require @babel/register",
    "watch": "npm run test -- --watch",
    "coverage": "nyc npm test"
  },
  "nyc": {
    "functions": 80,
    "lines": 80,
    "check-coverage": true,
    "reporter": [
      "text",
      "html"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvaldetaro/js-tdd.git"
  },
  "author": "Magno Valdetaro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mvaldetaro/js-tdd/issues"
  },
  "homepage": "https://github.com/mvaldetaro/js-tdd#readme",
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.8.3",
    "chai": "^4.2.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-mocha": "^6.2.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "mocha": "^6.2.2",
    "node-fetch": "^2.6.0",
    "nyc": "^15.0.0",
    "sinon": "^8.0.1",
    "sinon-chai": "^3.4.0",
    "sinon-stub-promise": "^4.0.0"
  }
